@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div id="myCarousel" class="carousel slide" data-ride="carousel">
	<ul class="carousel-indicators">
		<li data-target="#demo" data-slide-to="0" class="active"></li>
		<li data-target="#demo" data-slide-to="1"></li>
		<li data-target="#demo" data-slide-to="2"></li>
	</ul>
	<div class="carousel-inner">
		<div class="carousel-item active">
			<img src="~/css/images/2.jpg" class="mx-auto d-block" height=300 width=auto alt="Second slide">
			
		</div>
		<div class="carousel-item">
			<img src="~/css/images/1.jpeg" class="mx-auto d-block" height=300 width=auto alt="First slide">
		</div>
		<div class="carousel-item">
			<img src="~/css/images/3.jpeg" class="mx-auto d-block" height=300 width=auto alt ="Third slide">
		</div>
		
	</div>
	<a class="carousel-control-prev" href="#myCarousel" data-slide="prev">
		<span class="carousel-control-prev-icon"></span>
	</a>
	<a class="carousel-control-next" href="#myCarousel" data-slide="next">
		<span class="carousel-control-next-icon"></span>
	</a>
    
</div>
